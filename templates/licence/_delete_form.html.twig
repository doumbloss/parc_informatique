{# templates/licence/_delete_form.html.twig #}
<form method="post" action="{{ path('app_licence_delete', {'id': licence.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette licence ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ licence.id) }}">
    <button class="btn btn-danger btn-sm mt-2">Supprimer</button>
</form>
